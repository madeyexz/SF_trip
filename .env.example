# Required for RSS newsletter extraction (Beehiiv -> Firecrawl -> events)
FIRECRAWL_API_KEY=

# Required for rendering Google Maps in browser
GOOGLE_MAPS_BROWSER_KEY=

# Optional: Map ID from Google Cloud Console (required for Advanced Markers)
GOOGLE_MAPS_MAP_ID=

# Optional: server key used by /api/route (Routes API)
GOOGLE_MAPS_ROUTES_KEY=

# Optional: server key used by /api/sync for pre-geocoding events/places
# Falls back to GOOGLE_MAPS_SERVER_KEY or GOOGLE_MAPS_BROWSER_KEY if missing
GOOGLE_MAPS_GEOCODING_KEY=

# Optional: Convex persistence (if set, app reads/writes events from Convex)
CONVEX_URL=

# Required to unlock admin mode in Config (protects sync/source/config/shared planner writes)
APP_ADMIN_PASSWORD=

# Optional: session signing secret for admin cookie. Defaults to APP_ADMIN_PASSWORD if omitted.
APP_SESSION_SECRET=

# Optional
PORT=3000
LUMA_CALENDAR_URLS=https://luma.com/sf,https://luma.com/cerebralvalley_
RSS_INITIAL_ITEMS=1
RSS_MAX_ITEMS_PER_SYNC=3

# Trip date range (YYYY-MM-DD). All dates in range appear in the DayList.
TRIP_START=2026-02-07
TRIP_END=2026-02-14
